cmake_minimum_required(VERSION 2.8.3)
project(robot_stats)

find_package(catkin REQUIRED COMPONENTS
  nav_msgs
  roscpp
  rospy
  geometry_msgs
 std_msgs
)

catkin_package()

include_directories(
  ${catkin_INCLUDE_DIRS}
)

add_executable(vel_listener src/vel_listener.cpp)
add_dependencies(vel_listener nav_msgs_generate_messages_cpp)
 target_link_libraries(vel_listener
   ${catkin_LIBRARIES}
 )
